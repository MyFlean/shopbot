{
    "version": "7.2",
    "data_api_version": "3.0",
    "routing_model": {
        "ONBOARDING": [
            "COMPLETE"
        ],
        "COMPLETE": []
    },
    "screens": [
        {
            "id": "ONBOARDING",
            "title": "Flean Profile Setup",
            "data": {
                "show_custom_society": {
                    "type": "boolean",
                    "__example__": false
                },
                "societies": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "title": {
                                "type": "string"
                            }
                        }
                    },
                    "__example__": [
                        {
                            "id": "amrapali_sapphire",
                            "title": "Amrapali Sapphire"
                        },
                        {
                            "id": "parsvnath_prestige",
                            "title": "Parsvnath Prestige"
                        },
                        {
                            "id": "other",
                            "title": "Other"
                        }
                    ]
                },
                "genders": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "title": {
                                "type": "string"
                            }
                        }
                    },
                    "__example__": [
                        {
                            "id": "male",
                            "title": "üë® Male"
                        },
                        {
                            "id": "female",
                            "title": "üë© Female"
                        },
                        {
                            "id": "other",
                            "title": "üåà Other"
                        }
                    ]
                },
                "age_groups": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string"
                            },
                            "title": {
                                "type": "string"
                            }
                        }
                    },
                    "__example__": [
                        {
                            "id": "18_25",
                            "title": "üéì Young Explorer (18-25)"
                        },
                        {
                            "id": "26_35",
                            "title": "üöÄ Rising Star (26-35)"
                        },
                        {
                            "id": "36_45",
                            "title": "üíº Prime Time (36-45)"
                        },
                        {
                            "id": "46_55",
                            "title": "üèÜ Experienced Pro (46-55)"
                        }
                    ]
                }
            },
            "layout": {
                "type": "SingleColumnLayout",
                "children": [
                    {
                        "type": "Form",
                        "name": "onboarding_form",
                        "children": [
                            {
                                "type": "Image",
                                "src": "",
                                "height": 200
                            },
                            {
                                "type": "TextHeading",
                                "text": "üëã Great to meet you!"
                            },
                            {
                                "type": "TextBody",
                                "text": "We're setting up your Flean profile so you get relevant recommendations and the best local experience."
                            },
                            {
                                "type": "TextSubheading",
                                "text": "Please share:"
                            },
                            {
                                "type": "TextBody",
                                "text": "üè° Your Society - to check if we deliver in your area"
                            },
                            {
                                "type": "Dropdown",
                                "name": "society",
                                "label": "Society",
                                "required": true,
                                "data-source": "${data.societies}"
                            },
                            {
                                "type": "TextBody",
                                "text": "üí° If you selected 'Other', please specify below:"
                            },
                            {
                                "type": "TextInput",
                                "name": "custom_society",
                                "label": "Society name",
                                "input-type": "text",
                                "required": false
                            },
                            {
                                "type": "TextBody",
                                "text": "üöπ Your Gender - for more personalized options"
                            },
                            {
                                "type": "Dropdown",
                                "name": "gender",
                                "label": "Gender",
                                "required": true,
                                "data-source": "${data.genders}"
                            },
                            {
                                "type": "TextBody",
                                "text": "üéØ Your Age Group - for lifestyle-matched picks"
                            },
                            {
                                "type": "Dropdown",
                                "name": "age_group",
                                "label": "Age group",
                                "required": true,
                                "data-source": "${data.age_groups}"
                            },
                            {
                                "type": "Footer",
                                "label": "Save & Continue",
                                "on-click-action": {
                                    "name": "data_exchange",
                                    "payload": {
                                        "society": "${form.society}",
                                        "custom_society": "${form.custom_society}",
                                        "gender": "${form.gender}",
                                        "age_group": "${form.age_group}",
                                        "action_type": "submit_profile"
                                    }
                                }
                            },
                            {
                                "type": "TextCaption",
                                "text": "üîê Your data is secure and only used to improve your experience"
                            }
                        ]
                    }
                ]
            }
        },
        {
            "id": "COMPLETE",
            "title": "Welcome to Flean",
            "terminal": true,
            "layout": {
                "type": "SingleColumnLayout",
                "children": [
                    {
                        "type": "Image",
                        "src": "",
                        "height": 200
                    },
                    {
                        "type": "TextHeading",
                        "text": "üéâ Setup Complete!"
                    },
                    {
                        "type": "TextBody",
                        "text": "Thank you for joining Flean! You can now start asking us about products and get trustworthy recommendations to make smarter buying decisions."
                    },
                    {
                        "type": "TextSubheading",
                        "text": "How to use Flean:"
                    },
                    {
                        "type": "TextBody",
                        "text": "üí¨ Ask us about any product\n‚≠ê Get trustworthy recommendations\nüß† Make smarter buying decisions"
                    },
                    {
                        "type": "Footer",
                        "label": "Start Exploring",
                        "on-click-action": {
                            "name": "data_exchange",
                            "payload": {
                                "action_type": "complete_flow"
                            }
                        }
                    }
                ]
            }
        }
    ]
}